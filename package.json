{
  "name": "mirror",
  "version": "1.0.0",
  "description": "Unlimited Basketball Prediction AI - The Next Big Thing",
  "main": "src/index.js",
  "bin": {
    "mirror": "./src/index.js"
  },
  "type": "commonjs",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "postinstall": "chmod +x src/index.js",
    "setup": "node scripts/setup.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:status": "pm2 status",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "docker:build": "docker build -t mirror .",
    "docker:run": "docker run -it --env-file .env mirror",
    "prod:start": "NODE_ENV=production pm2 start ecosystem.config.js",
    "prod:stop": "pm2 stop ecosystem.config.js && pm2 delete ecosystem.config.js"
  },
  "keywords": [
    "basketball",
    "prediction",
    "ai",
    "scraping",
    "telegram",
    "openrouter"
  ],
  "author": "Mirror AI",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.0",
    "helmet": "^7.1.0",
    "inquirer": "^9.2.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.43",
    "node-telegram-bot-api": "^0.66.0",
    "openai": "^4.28.0",
    "ora": "^8.0.0",
    "playwright": "^1.43.0",
    "puppeteer": "^22.0.0",
    "telegraf": "^4.16.0"
  }
}
